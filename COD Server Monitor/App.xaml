<Application x:Class="COD_Server_Monitor.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:COD_Server_Monitor"
             xmlns:skins="clr-namespace:COD_Server_Monitor"
             StartupUri="Windows/MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <skins:SkinResourceDictionary LightSource="Skins/LightTheme.xaml" DarkSource="Skins/DarkTheme.xaml"/>
                <ResourceDictionary Source="Skins/MyTheme.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
